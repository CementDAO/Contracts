{
    "name": "contracts",
    "version": "1.0.0",
    "description": "The MIXR contracts",
    "repository": {
        "type": "git",
        "url": "https://github.com/CementDAO/Contracts.git"
    },
    "main": "truffle-config.js",
    "directories": {
        "test": "test"
    },
    "scripts": {
        "deploy": "truffle deploy",
        "develop": "truffle develop",
        "test": "truffle test --network test",
        "lint:solidity": "solium -d contracts",
        "lint:js": "eslint *.js .",
        "lint": "npm run lint:solidity && npm run lint:js",
        "security": "docker run --memory=3g -v $(pwd):/tmp -w \"/tmp/\" mythril/myth --truffle",
        "coverage": "solidity-coverage"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "openzeppelin-solidity": "2.1.2",
        "bignumber.js": "8.0.2"
    },
    "devDependencies": {
        "chai": "^4.2.0",
        "chai-bignumber": "^3.0.0",
        "eslint": "^5.13.0",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-plugin-import": "^2.16.0",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-react": "^7.12.4",
        "eth-gas-reporter": "^0.1.12",
        "ethlint": "1.2.3",
        "ganache-cli": "6.3.0",
        "solidity-coverage": "obernardovieira/solidity-coverage#bf55aed",
        "truffle": "5.0.0"
    }
